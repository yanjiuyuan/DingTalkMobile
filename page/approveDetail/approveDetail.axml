// page.axml
<list>
  <view slot="round_icon">兴桐</view>
  <block a:for="{{items}}">
    <list-item
      key="item-{{index}}"
	  index="{{index}}"
      last="{{index === (items.length - 1)}}"
    >
      {{item.title}}
      <view class="am-list-brief">{{item.brief}}</view>
    </list-item>
  </block>
  <view slot="footer">列表尾部</view>
</list>